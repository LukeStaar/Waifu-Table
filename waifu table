CREATE TABLE waifu(
idwaifu INT PRIMARY KEY,
nome varchar(50),
anime varchar(50),
nivelDeAmor varchar (10) ,
dono varchar(50)
);

delete from waifu where idwaifu=2;
update waifu set nivelDeAmor='666' where idwaifu=3; 

